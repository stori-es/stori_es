<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .iconWrapper {
            font-size: 31px;
            margin-right: 5px;
            cursor: pointer;
        }
    </ui:style>

    <g:HTMLPanel ui:field="main">
        <div class="{style.iconWrapper}">
            <i class="icon-plus-sign" style="display: block;"/>
        </div>
    </g:HTMLPanel>
</ui:UiBinder>
