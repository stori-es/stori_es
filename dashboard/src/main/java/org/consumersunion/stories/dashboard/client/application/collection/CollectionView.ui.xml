<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:with field="resource" type="org.consumersunion.stories.dashboard.client.resource.Resources"/>

    <g:HTMLPanel styleName="{resource.generalStyleCss.mainContent}">
        <g:SimplePanel ui:field="mainCard"/>
        <g:SimplePanel ui:field="navBar"/>
        <g:SimplePanel ui:field="newContent"/>
        <g:SimplePanel ui:field="navContent" visible="false" styleName="{resource.generalStyleCss.navContent}"/>

        <div class="{resource.generalStyleCss.clearBoth}"/>
    </g:HTMLPanel>
</ui:UiBinder>
